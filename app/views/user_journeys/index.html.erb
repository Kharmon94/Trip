<p id="notice"><%= notice %></p>

<h1>User Journeys</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_journeys.each do |user_journey| %>
      <tr>
        <td><%= link_to 'Show', user_journey %></td>
        <td><%= link_to 'Edit', edit_user_journey_path(user_journey) %></td>
        <td><%= link_to 'Destroy', user_journey, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Journey', new_user_journey_path %>
